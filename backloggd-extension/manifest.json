{
  "manifest_version": 3,
  "name": "Backloggd SearchBar",
  "version": "1.1.0",
  "description": "Browse and cache games from Backloggd profiles using this search bar.",
  "permissions": [
    "storage"
  ],
  "homepage_url": "https://github.com/alexgbor/backloggd-improved-searchbar",
  "content_scripts": [
    {
      "matches": [
        "https://backloggd.com/*",
        "https://www.backloggd.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}